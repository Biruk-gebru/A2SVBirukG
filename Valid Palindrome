class Solution:
    def isPalindrome(self, s: str) -> bool:
        s = s.lower()
        wor = ''
        for i in range(len(s)):
            if s[i]>='a' and s[i]<='z'or (s[i]>='0' and s[i]<='9'):
                wor += s[i]
        w=list(wor)
        r=w[::-1]
        if r==w:
            return(True)
        else:
            return(False)
